<mvc:View controllerName="zov.controller.NavBarView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns="sap.m">
        <Toolbar id="toolbar">
            <ToolbarSpacer id="toolbar1"/>

            <Button icon="sap-icon://home" text="Home" press="onHome" class="sapUiMediumMargin" />

            <ToolbarSeparator/>

            <Button icon="sap-icon://create" text="{i18n>createOrder}" press="onCreateOv" class="sapUiMediumMargin"/>

            <ToolbarSeparator/>

            <Button icon="sap-icon://create-form" text="{i18n>createOrderDeep}" press="onCreateDeepOv" class="sapUiMediumMargin"/>

            <ToolbarSeparator/>

            <Button icon="sap-icon://search" text="{i18n>viewOrder}" press="onReadOv" class="sapUiMediumMargin" />

            <ToolbarSeparator/>

            <Button icon="sap-icon://edit" text="{i18n>modifyOrder}" press="onEditOv" class="sapUiMediumMargin"/>

            <ToolbarSeparator/>

            <Button icon="sap-icon://delete" text="{i18n>deleteOrder}" press="onDeleteOv" class="sapUiMediumMargin"/>

            <ToolbarSpacer />
        </Toolbar>
</mvc:View>