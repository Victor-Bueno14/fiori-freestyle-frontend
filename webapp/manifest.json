{
  "_version": "1.64.0",
  "sap.app": {
    "id": "zov",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "4fc8ab6c-b96c-49d9-ac0d-16930dec69fe"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZOV_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "usuarios": {
        "uri": "model/Usuarios.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.124.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zov.i18n.i18n"
        }
      },
      "usuarios": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "usuarios"
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "zov.view",
        "async": true,
        "viewPath": "zov.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "RouteView2",
          "pattern": "view2",
          "target": [
            "TargetView2"
          ]
        },
        {
          "name": "RouteCreateOv",
          "pattern": "criarOv",
          "target": [
            "TargetCreateOv"
          ]
        },
        {
          "name": "RouteCreateDeepOv",
          "pattern": "criarDeepOv",
          "target": [
            "TargetCreateDeepOv"
          ]
        },
        {
          "name": "RouteReadOv",
          "pattern": "lerOV",
          "target": [
            "TargetReadOv"
          ]
        },
        {
          "name": "RouteEditOv",
          "pattern": "modificarOV",
          "target": [
            "TargetEditOv"
          ]
        },
        {
          "name": "RouteDeleteOv",
          "pattern": "deletarOV",
          "target": [
            "TargetDeleteOv"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "id": "View1",
          "name": "View1"
        },
        "TargetView2": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "id": "View2",
          "name": "View2"
        },
        "TargetCreateOv": {
          "viewType": "XML",
          "transition":"slide",
          "clearControlAggregation": false,
          "viewId": "CreateOvView",
          "viewName": "CreateOvView"
        },
        "TargetCreateDeepOv": {
          "viewType": "XML",
          "transition":"slide",
          "clearControlAggregation": false,
          "viewId": "CreateDeepOvView",
          "viewName": "CreateDeepOvView"
        },
        "TargetReadOv": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ReadOvView",
          "viewName": "ReadOvView"
        },
        "TargetEditOv": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "EditOvView",
          "viewName": "EditOvView"
        },
        "TargetDeleteOv": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "DeleteOvView",
          "viewName": "DeleteOvView"
        }
      }
    },
    "rootView": {
      "viewName": "zov.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
